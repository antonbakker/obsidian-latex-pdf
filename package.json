{
  "name": "obsidian-latex-pdf",
  "version": "1.0.5",
  "description": "Obsidian plugin to export notes to PDF via Pandoc and LaTeX templates.",
  "main": "main.js",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=main.js --format=cjs --platform=node --external:obsidian --sourcemap",
    "build:server": "esbuild src/server.ts --bundle --platform=node --format=cjs --outfile=server.js",
    "dev": "npm run build -- --watch",
    "lint": "echo 'TODO: add eslint'",
    "test": "vitest",
    "test:unit": "vitest",
    "test:cdk": "vitest tests/obsidian-latex-pdf-stack.test.ts",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:github": "node scripts/gh-release.js"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "aws-cdk-lib": "^2.154.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.24.0",
    "obsidian": "^1.5.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": ">=3.971.0",
    "@aws-sdk/s3-request-presigner": ">=3.971.0",
    "@fastify/multipart": ">=9.3.0",
    "@types/jsonwebtoken": ">=9.0.10",
    "fastify": ">=5.7.1",
    "jsonwebtoken": ">=9.0.3"
  }
}
